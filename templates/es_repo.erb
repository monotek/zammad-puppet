# elasticsearch repo file managed by puppet
# do not change
<% if @operatingsystem =~ /^(CentOS|RedHat)$/ %>
[elasticsearch-<%= @zammad::es_version %>]
name=Elasticsearch repository for <%= @zammad::es_version %> packages
baseurl=https://artifacts.elastic.co/packages/<%= @zammad::es_version %>/yum
gpgcheck=1
gpgkey=https://artifacts.elastic.co/GPG-KEY-elasticsearch
enabled=1
autorefresh=1
type=rpm-md
<% elsif @operatingsystem =~ /^(Debian|Ubuntu)$/ %>
deb https://artifacts.elastic.co/packages/<%= @zammad::es_version %>/apt stable main
<% elsif @operatingsystem =~ /^(SLES|SUSE)$/ %>
[elasticsearch]
name=Repository for elasticsearch <%= @zammad::es_version %>
baseurl=https://artifacts.elastic.co/packages/<%= @zammad::es_version %>/yum
enabled=1
<% end %>
